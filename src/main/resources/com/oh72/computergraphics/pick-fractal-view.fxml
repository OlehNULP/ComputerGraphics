<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="214.0" prefWidth="300.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.oh72.computergraphics.PickFractalController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Button fx:id="dragonCurve" mnemonicParsing="false" prefWidth="100.0" text="Dragon Curve"
                    GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
            <Button fx:id="kochCurve" mnemonicParsing="false" prefWidth="100.0" text="Koch Curve" textAlignment="CENTER"
                    GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
            <Button fx:id="colorScheme" layoutX="30.0" layoutY="43.0" mnemonicParsing="false" prefWidth="100.0"
                    text="Color Scheme" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                    GridPane.valignment="CENTER"/>
            <Button fx:id="triangle" layoutX="10.0" layoutY="43.0" mnemonicParsing="false" prefWidth="100.0"
                    text="Triangle" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                    GridPane.valignment="CENTER"/>
        </children>
    </GridPane>
</VBox>
